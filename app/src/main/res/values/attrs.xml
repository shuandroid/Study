<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension"/>
        <attr name="civ_border_color" format="color"/>
        <attr name="civ_border_overlay" format="boolean"/>
        <attr name="civ_fill_color" format="color"/>

    </declare-styleable>

    <declare-styleable name="Indicator">
        <attr name="circle_color" format="color"/>
        <attr name="fill_color" format="color"/>
        <attr name="stroke_width" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="WechatTab">
        <attr name="pstsIndicatorColorWe" format="color"/>
        <attr name="pstsUnderlineColorWe" format="color"/>
        <attr name="pstsDividerColorWe" format="color"/>
        <attr name="pstsIndicatorHeightWe" format="dimension"/>
        <attr name="pstsUnderlineHeightWe" format="dimension"/>
        <attr name="pstsDividerPaddingWe" format="dimension"/>
        <attr name="pstsTabPaddingLeftRightWe" format="dimension"/>
        <attr name="pstsScrollOffsetWe" format="dimension"/>
        <attr name="pstsTabBackgroundWe" format="reference"/>
        <attr name="pstsShouldExpandWe" format="boolean"/>
        <attr name="pstsTextAllCapsWe" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="TabView">
        <attr name="text_size" format="dimension"/>
        <attr name="text_normal_color" format="color"/>
        <attr name="text_select_color" format="color"/>
        <attr name="item_padding" format="dimension"/>
    </declare-styleable>

</resources>
